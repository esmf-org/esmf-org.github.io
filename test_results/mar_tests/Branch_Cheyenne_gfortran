                       [--dst_loc center|corner]
                       [--method|-m bilinear|patch|neareststod|nearestd=
tos|conserve|conserve2nd]
                       [--pole|-p all|none|teeth|<N>]
                       [--ignore_unmapped|-i]
                       [--ignore_degenerate]
